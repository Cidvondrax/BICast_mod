{
  "multipart": [
    {
      "apply": {
        "model": "discord_mod:block/crackedglass_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "discord_mod:block/crackedglass_noside",
        "y": 270
      }
    }
  ]
}